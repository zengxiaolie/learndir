void run(left,right)                                             			//电机正转
{
	SetMoto(1,left*fv);
	SetMoto(0,right*fv);
}

void runb(left,right)                                           			//电机反转
{
	SetMoto(0,-left*fv);
	SetMoto(1,-right*fv);
}

void stop()                                                 				//停止
{
        run(0,0);
}

void fan()                                                   				//风扇转
{
        SetMoto(2,-100);
}

void fanb()                                                 				//风扇反转
{
	SetMoto(2,100);
}

void fanstop()                                                 				//风扇停止
{
        SetMoto(2,0);
}

