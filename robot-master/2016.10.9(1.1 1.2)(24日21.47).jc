void mg_left(int pwm_1,int pwm_2)					//沿左墙
{
        if(DI(3)==have)
	{
		run(68,40);
	}      
	else  if(DI(2)==have && DI(11)==have)
        {
                run(70,70);//65 68
	}               
	else if(DI(2)==have)
	{
                run(68,50);
	}
	else 
        {
		run(pwm_1,pwm_2);
        }
}

