void home3()
{
        fv=0.8;
        while(AI_MAX(12)<surf12)
        {
                mg_rightb_1(40,15);
	}
        //stop();wait(10000);
        float t3=seconds();
        while(seconds()-t3<0.38)
        {
                mg_rightb_1(40,5);
	}
        //stop();wait(10000);
        while(AI_MAX(13)<surf13-200)
        {
                mg_leftb_1(10,40);
	}
        stop();wait(10000);
}

void home2()
{
        fv=0.8;
        while(AI_MAX(12)<surf12)
        {
                mg_right_1(60,25);//55 8
	}
        //stop();wait(10000);
         while(DI(1)==no)
        {
                 mg_left_1(10,65);
	 }
         //stop();wait(10000);
        float t2=seconds();
        while(seconds()-t2<1.6)
        {
                mg_left_1(15,55);
	}
        //stop();wait(10000);
        for(i=40;i>0;i--)
        {
                run(i,i);
                wait(0.003);
        }
        for(i=0;i<40;i++)
        {
                runb(i,i);
                wait(0.003);
        }
        while(AI_MAX(13)<surf13 && DI(1)==no)
        {
                mg_leftb_1(13,55);
	}
         for(i=40;i>0;i--)
        {
                runb(i,i);
                wait(0.003);
        }
         stop();wait(10000);
 }


